{
  "name": "@mapbox/underreact",
  "version": "0.1.0",
  "description": "Minimal, extensible React app build system that you won't need to eject",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "lint-md": "remark-preset-davidtheclark",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "remark-preset-davidtheclark",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "author": "Mapbox",
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "@mapbox/fusspot": "^0.2.1",
    "address": "^1.0.3",
    "assets-webpack-plugin": "^3.8.4",
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "concat-with-sourcemaps": "^1.1.0",
    "connect-history-api-fallback": "^1.5.0",
    "cpy": "^7.0.1",
    "del": "^3.0.0",
    "findup-sync": "^2.0.0",
    "fs-extra": "^7.0.0",
    "get-port": "^3.2.0",
    "live-server": "^1.2.0",
    "lodash": "^4.17.10",
    "object-assign": "^4.1.1",
    "pify": "^3.0.0",
    "postcss": "^6.0.23",
    "postcss-csso": "^3.0.0",
    "postcss-url": "^7.3.2",
    "promise": "^8.0.1",
    "remark-preset-davidtheclark": "^0.8.1",
    "resolve-pkg": "^1.0.0",
    "rev-hash": "^2.0.0",
    "time-stamp": "^2.0.0",
    "uglify-js": "^3.4.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-join": "^4.0.0",
    "webpack": "^4.12.2",
    "webpack-format-messages": "^2.0.1",
    "whatwg-fetch": "^2.0.4",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@mapbox/eslint-config-mapbox": "^1.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.6",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "bin": {
    "underreact": "bin/underreact.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/underreact.git"
  },
  "keywords": [
    "react",
    "spa",
    "app",
    "build"
  ],
  "jest": {
    "transform": {},
    "clearMocks": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.js",
      "!<rootDir>/test/**"
    ]
  },
  "bugs": {
    "url": "https://github.com/mapbox/underreact/issues"
  },
  "homepage": "https://github.com/mapbox/underreact#readme"
}
